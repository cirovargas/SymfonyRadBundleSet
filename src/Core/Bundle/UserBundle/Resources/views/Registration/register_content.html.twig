{% trans_default_domain 'FOSUserBundle' %}
<h1 class="form-header">Create your Account</h1>

<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" id="signup-form_id" class="panel">
    {{ form_widget(form) }}
    

    <div class="form-actions">
            <input type="submit" value="{{ 'registration.submit'|trans }}" class="btn btn-primary btn-lg btn-block">
    </div>
</form>