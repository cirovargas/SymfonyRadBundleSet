    <script type="text/javascript"> window.jQuery || document.write('<script src="{{ asset('admin2/libs/jquery/dist/jquery.min.js') }}">'+"<"+"/script>");</script>

    <script>var pxInit = [];</script>
{% endblock %}
{% block body_class %}{% endblock %}

{% block body %}
    <div class="page-signin-header p-a-2 text-sm-center bg-white">
        <a class="px-demo-brand px-demo-brand-lg text-default" href="{{ path('admin_home') }}">
            <span class="px-demo-logo bg-primary m-t-0"><img src="{{ asset('apple-touch-icon.png') }}" height="24px" alt="" style="margin-top: -4px;"></span>
            <strong>Projeto </strong> Symfony
        </a>
        {% if buttom_login is defined and buttom_login == true %}
        <a href="{{ path('fos_user_security_login') }}" class="btn btn-primary">{{ 'layout.login'|trans }}</a>
        {% endif %}
        {% if buttom_logout is defined and buttom_logout == true %}
        <a href="{{ path('logout') }}" class="btn btn-primary">{{ 'layout.logout'|trans }}</a>
        {% endif %}
        {% if buttom_register is defined and buttom_register == true %}
        <a href="{{ path('fos_user_registration_register') }}" class="btn btn-primary">{{ 'layout.register'|trans }}</a>
        {% endif %}
    </div>
